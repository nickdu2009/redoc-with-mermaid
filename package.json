{
  "name": "redoc-with-mermaid",
  "version": "1.0.0",
  "description": "A React component that integrates Redoc with automatic Mermaid diagram rendering",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:lib": "vite build --config vite.config.lib.ts && tsc --project tsconfig.build.json",
    "preview": "vite preview",
    "prepublishOnly": "npm run build:lib"
  },
  "keywords": [
    "redoc",
    "mermaid",
    "openapi",
    "swagger",
    "api-documentation",
    "react",
    "diagram"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "peerDependencies": {
    "mermaid": ">=10.0.0",
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0",
    "redoc": ">=2.0.0",
    "styled-components": ">=5.0.0 || >=6.0.0"
  },
  "peerDependenciesMeta": {
    "styled-components": {
      "optional": false
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "mermaid": "^10.9.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "redoc": "^2.5.2",
    "styled-components": "^6.1.13",
    "typescript": "~5.6.2",
    "vite": "^6.0.3",
    "vite-plugin-dts": "^4.3.0"
  }
}
